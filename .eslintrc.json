{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "node/no-unsupported-features/es-syntax": 0,
        "node/no-unpublished-import": 0,
        "node/no-unpublished-require": 0
    }
}